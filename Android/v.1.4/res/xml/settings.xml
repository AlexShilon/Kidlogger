<PreferenceScreen
	android:key="dev_settings" 
	android:title="@string/pref_devsettings_title">	
	<Preference android:key="goToSite" android:layout="@layout/preference_layout"></Preference>
	<CheckBoxPreference 
		android:key="uploadKey" 
		android:defaultValue="false" android:title="@string/pref_uplogs_title">
 	</CheckBoxPreference>
 	<EditTextPreference 
 		android:key="deviceField" 
 		android:dependency="uploadKey" android:title="@string/pref_devid_title" android:summary="@string/pref_devid_summ">
	</EditTextPreference>
	<EditTextPreference 
		android:key="freqSend" 
		android:dependency="uploadKey" 
		android:title="@string/pref_delivinterval_title" android:summary="@string/pref_delivinterval_summ" android:dialogMessage="@string/pref_delivinterval_dialog" android:numeric="integer" android:defaultValue="10">
	</EditTextPreference>
	<net.kidlogger.kidlogger.SendTestReport
		android:key="testRep" android:summary="My preference summary" android:title="My preference" android:dependency="uploadKey"/>

</PreferenceScreen>
<EditTextPreference android:key="accessKey" android:defaultValue="123456" android:title="@string/pref_accesspass_title" android:summary="@string/pref_accesspass_summ" android:dialogMessage="@string/pref_accesspass_dialog"></EditTextPreference>
</PreferenceScreen>
