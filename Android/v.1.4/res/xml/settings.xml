<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
<PreferenceScreen android:title="@string/pref_coordinates_sett">
	<CheckBoxPreference 
		android:key="logGps" 
		android:defaultValue="false" 
		android:summary="@string/pref_gps_summ" 
		android:title="@string/pref_gps_title">
	</CheckBoxPreference>
	<CheckBoxPreference 
		android:defaultValue="true" 
		android:key="logCell" 
		android:summary="@string/pref_cellloc_summ" 
		android:title="@string/pref_cellloc_title" 
		android:dependency="logGsm">
	</CheckBoxPreference>
	<EditTextPreference 
		android:key="refreshGPS" 
		android:defaultValue="5" android:title="@string/pref_gpsrefresh_title" android:summary="@string/pref_gpsrefresh_summ" android:dialogMessage="@string/pref_gpsrefresh_dialog">
	</EditTextPreference>
	<EditTextPreference 
		android:key="minDist" 
		android:defaultValue="20" android:title="@string/pref_gpsdist_title" android:summary="@string/pref_gpsdist_summ" android:dialogMessage="@string/pref_gpsdist_dialog">
	</EditTextPreference>
</PreferenceScreen>
